<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
        http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="My first report">
    <parameter name="mostLent" class="java.lang.String"/>
    <parameter name="mostReturned" class="java.lang.String"/>
    <parameter name="mostTotalLent" class="java.lang.String"/>
    <parameter name="generated" class="java.lang.String"/>
    <queryString>
        <![CDATA[]]>
    </queryString>
    <field name="month">
        <fieldDescription>
            <![CDATA[month]]>
        </fieldDescription>
    </field>
    <field name="lent">
        <fieldDescription>
            <![CDATA[lent]]>
        </fieldDescription>
    </field>
    <field name="returned">
        <fieldDescription>
            <![CDATA[returned]]>
        </fieldDescription>
    </field>
    <field name="totalLent">
        <fieldDescription>
            <![CDATA[totalLent]]>
        </fieldDescription>
    </field>
    <background>
        <band splitType="Stretch"/>
    </background>
    <title>
        <band height="42" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="0" width="50" height="42"/>
                <textElement textAlignment="Center">
                    <font size="20" isBold="true"/>
                </textElement>
                <text>
                    <![CDATA[Lending report]]>
                </text>
            </staticText>
        </band>
    </title>
    <columnHeader>
        <band height="100" splitType="Stretch">
            <staticText>
                <reportElement x="0" y="0" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Most lent in:]]></text>
            </staticText>
            <textField>
                <reportElement x="100" y="0" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.Integer">
                    <![CDATA[$P{mostLent}]]>
                </textFieldExpression>
            </textField>
            <staticText>
                <reportElement x="0" y="15" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Most returned in:]]></text>
            </staticText>
            <textField>
                <reportElement x="100" y="15" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.Integer">
                    <![CDATA[$P{mostReturned}]]>
                </textFieldExpression>
            </textField>
            <staticText>
                <reportElement x="0" y="30" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Most total lent in:]]></text>
            </staticText>
            <textField>
                <reportElement x="100" y="30" width="200" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$P{mostTotalLent}]]>
                </textFieldExpression>
            </textField>
            <staticText>
                <reportElement x="300" y="0" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Generated on:]]></text>
            </staticText>
            <textField>
                <reportElement x="400" y="0" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$P{generated}]]>
                </textFieldExpression>
            </textField>
            <staticText>
                <reportElement x="0" y="70" width="50" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Month]]></text>
            </staticText>
            <staticText>
                <reportElement x="100" y="70" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Lent]]></text>
            </staticText>
            <staticText>
                <reportElement x="200" y="70" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Returned]]></text>
            </staticText>
            <staticText>
                <reportElement x="300" y="70" width="100" height="20"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <text><![CDATA[Total lent]]></text>
            </staticText>
        </band>
    </columnHeader>
    <detail>
        <band height="20" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="50" height="15"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{month}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="100" y="0" width="100" height="15"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{lent}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="200" y="0" width="100" height="15"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{returned}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="300" y="0" width="100" height="15"/>
                <textElement textAlignment="Left" verticalAlignment="Middle"/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$F{totalLent}]]>
                </textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>